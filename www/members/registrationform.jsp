                                <input name="Name" type="text" class="textbox" id="Name" maxlength="100" />
                              </td>
                            </tr>
                            <tr>
                              <td width="140">Giới tính</td>
                              <td width="300">Nam 
                                <input name="Gender" type="radio" value="1" checked="checked" />
                                Nữ  
                                <input type="radio" name="Gender" value="0" />
                                                           </td>
                            </tr>
                            <tr>
                              <td width="140">Ngày sinh</td>
                              <td width="300">
                                <select name="Day" class="listbox" id="Day">
									
                                  <option value="-1" selected="selected">--Ngày--</option>
                                  <%
	for (int i = 1; i <= 31; i++) {
%>
                                  									
									
                                  <option value="<%= i %>"><%= i %></option>
                                  <%
	}
%>
                                  									
                                </select>
/ 
                                <select name="Month" class="listbox" id="Month">
									
                                  <option value="-1" selected="selected">--Tháng--</option>
                                  <%
	for (int i = 0; i < 12; i++) {
%>
                                  									
									
                                  <option value="<%= i %>"><%= i+1 %></option>
                                  <%
	}
%>
                                  									
                                </select>
/ 
                                <select name="Year" class="listbox" id="Year">
									
                                  <option value="-1" selected="selected">--Năm--</option>
                                  <%@ page import="java.text.SimpleDateFormat" %>
                                  <%
	SimpleDateFormat sdf = new SimpleDateFormat("yyyy");
	int now = Integer.parseInt(sdf.format(new Date()));
	for (int i = 1900; i <= now; i++) {
%>
                                  									
									
                                  <option value="<%= i %>"><%= i %></option>
                                  <%
	}
%>
                                  									
                                </select>
                                                                                               
